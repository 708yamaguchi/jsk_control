<launch>
  <arg name="NLOPT_IK_NODE_ROBOT" default=""/>

  <node pkg="eus_nlopt"
        type="nlopt-ik-node.l"
        name="nlopt_ik_node1"
        output="screen">
    <env name="NLOPT_IK_NODE_ROBOT" value="${NLOPT_IK_NODE_ROBOT}" />
    <env name="NLOPT_IK_NODE_ALGORITHM" value="3" />
  </node>

  <node pkg="eus_nlopt"
        type="nlopt-ik-node.l"
        name="nlopt_ik_node2"
        output="screen">
    <env name="NLOPT_IK_NODE_ROBOT" value="${NLOPT_IK_NODE_ROBOT}" />
    <env name="NLOPT_IK_NODE_ALGORITHM" value="8" />
  </node>

  <node pkg="eus_nlopt"
        type="nlopt-ik-node.l"
        name="nlopt_ik_node3"
        output="screen">
    <env name="NLOPT_IK_NODE_ROBOT" value="${NLOPT_IK_NODE_ROBOT}" />
    <env name="NLOPT_IK_NODE_ALGORITHM" value="30" />
  </node>

  <node pkg="eus_nlopt"
        type="nlopt-ik-manager.l"
        name="nlopt_ik_manager"
        output="screen">
  </node>

</launch>
